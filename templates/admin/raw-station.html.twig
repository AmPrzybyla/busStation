<tr>
    <td>
            {% if notification.readed == true %}
                przeczytane
            {% else %}
                nieprzeczytane
            {% endif %}
    </td>
    <td>{{ notification.title.name }}</td>
    <td>{{ notification.text }}</td>
    <td><a href="{{ path('admin_view', {'id':notification.id}) }}" class="btn btn-primary">View</a></td>
</tr>